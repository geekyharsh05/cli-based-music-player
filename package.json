{
  "name": "terminal-music-player",
  "version": "1.0.0",
  "description": "A terminal-based music player that streams from YouTube",
  "main": "bin/cli.js",
  "bin": {
    "tmusic": "./bin/cli.js"
  },
  "scripts": {
    "start": "node bin/cli.js",
    "dev": "nodemon bin/cli.js",
    "build": "pkg . --out-path ./dist",
    "audit-fix": "npm audit fix",
    "security-check": "npm audit --audit-level moderate"
  },
  "keywords": [
    "music",
    "terminal",
    "cli",
    "youtube",
    "player",
    "ytmusic",
    "audio",
    "streaming"
  ],
  "type": "module",
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/terminal-music-player.git"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "inquirer": "^10.2.2",
    "ora": "^8.1.1",
    "terminal-music-player": "^1.0.0",
    "ytmusic-api": "^5.3.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.7"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "preferGlobal": true,
  "overrides": {
    "semver": "^7.6.3",
    "follow-redirects": "^1.15.6",
    "axios": "^1.7.7"
  }
}
